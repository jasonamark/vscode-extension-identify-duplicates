{
	"name": "identify-duplicates",
	"displayName": "Identify Duplicates",
	"description": "Identifies duplicate objects defined in a repository.",
	"version": "1.0.0",
	"publisher": "JasonMark",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jasonamark/vscode-extension-identify-duplicates"
	},
  "engines": {
    "vscode": "^1.90.0",
    "node": ">=20.15.0"
  },
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:json",
		"onLanguage:jsonc"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "duplicate-objects",
					"title": "Duplicate Objects",
					"icon": "media/dep.svg"
				}
			]
		},
		"views": {
			"duplicate-objects": [
				{
					"id": "duplicateObjects",
					"name": "Duplicate Objects",
					"icon": "media/dep.svg"
				}
			]
		},
		"commands": [
			{
				"command": "identify-duplicates.openFile",
				"title": "Treeview Item Clicked"
			}
		]
	},
	"scripts": {
		"vscode:package": "npm run compile && vsce package",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint \"src/**/*.ts\""
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.26.0",
		"typescript": "^5.3.2"
	},
	"dependencies": {
		"minimist": "^1.2.6",
		"mkdirp": "^0.5.1",
		"rimraf": "^2.6.2"
	}
}
