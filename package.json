{
  "name": "identify-duplicates",
  "displayName": "Duplicate Objects",
  "description": "Identifies duplicate objects defined in a repository.",
  "version": "1.0.13",
  "publisher": "JasonMark",
  "private": true,
  "license": "MIT",
  "icon": "resources/identify-duplicates.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/jasonamark/vscode-extension-identify-duplicates"
  },
  "engines": {
    "vscode": "^1.90.0",
    "node": ">=20.15.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "find",
    "identify",
    "duplicate",
    "identical",
    "objects",
    "css",
    "methods",
    "interface",
    "enum",
    "redundant",
    "dry"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "menus": {
      "view/title": [
        {
          "command": "identify-duplicates.refresh",
          "when": "view == identicalObjects",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "identical-objects",
          "title": "Duplicate Objects",
          "icon": "resources/identify-duplicates.svg"
        }
      ]
    },
    "views": {
      "identical-objects": [
        {
          "type": "webview",
          "id": "identicalObjects",
          "name": "Duplicate Objects"
        }
      ]
    },
    "commands": [
      {
        "command": "identify-duplicates.openFile",
        "title": "Treeview Item Clicked"
      },
      {
        "command": "identify-duplicates.refresh",
        "title": "Refresh",
        "icon": {
          "light": "resources/refresh.svg",
          "dark": "resources/refresh.svg"
        }
      }
    ]
  },
  "scripts": {
    "vscode:package": "npm run package && vsce package",
    "format": "npx prettier . --write",
    "watch": "npm-run-all --parallel watch:*",
    "watch:ext": "webpack --mode development --watch --config ./webpack/extension.config.js",
    "watch:wv": "webpack serve --mode development --config ./webpack/webview.config.js",
    "package": "npm run clean && npm-run-all --parallel package:*",
    "package:ext": "webpack --mode production --config ./webpack/extension.config.js",
    "package:wv": "webpack --mode production --config ./webpack/webview.config.js",
    "clean": "rimraf dist",
    "compile-tests": "tsc -p . --outDir dist",
    "lint": "eslint \"src/**/*.ts\""
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "css": "~3.0.0",
    "file-loader": "~6.2.0",
    "fs": "~0.0.1-security",
    "path": "~0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "~5.5.4"
  },
  "devDependencies": {
    "@types/node": "20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/vscode": "^1.90.0",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vscode/webview-ui-toolkit": "~1.4.0",
    "css-loader": "~7.1.2",
    "eslint": "^8.26.0",
    "npm-run-all": "~4.1.5",
    "postcss-loader": "~8.1.1",
    "prettier": "3.3.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "webpack": "^5.92.1",
    "webpack-cli": "~5.1.4",
    "webpack-dev-server": "~5.0.4"
  }
}
